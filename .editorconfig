# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
# Unix-style newlines with a newline ending every file
end_of_line = lf
insert_final_newline = true
# Set default charset
charset = utf-8

[{*.js,*.json}]
# Lots of stuff wants 2 spaces (anything Prettier would touch, for example)
indent_style = space
indent_size = 2
